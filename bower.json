{
  "name": "lobby-element",
  "version": "0.0.1",
  "authors": [
    "Aengus Bates <aengusbates@gmail.com>"
  ],
  "description": "A simple contact form allowing users to lobby their representatives. The form allows users to filter representatives by constituency and to edit and send them an email.",
  "keywords": [
    "web-component",
    "polymer",
    "lobby",
    "email",
    "contact form"
  ],
  "main": "lobby-element.html",
  "license": "MIT",
  "homepage": "https://github.com/aengusbates/lobby-element/",
  "ignore": [
    "/.*",
    "/test/"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1.2.0",
    "paper-styles": "PolymerElements/paper-styles#~1.0.12",
    "paper-button": "PolymerElements/paper-button#~1.0.8",
    "paper-input": "PolymerElements/paper-input#~1.1.0",
    "paper-listbox": "polymerelements/paper-listbox#~1.0.0",
    "paper-item": "PolymerElements/paper-item#~1.1.1",
    "paper-menu": "PolymerElements/paper-menu#~1.2.0",
    "paper-dropdown-menu": "PolymerElements/paper-dropdown-menu#~1.1.0",
    "iron-ajax": "PolymerElements/iron-ajax#~1.1.0",
    "paper-checkbox": "PolymerElements/paper-checkbox#~1.0.14",
    "gold-email-input": "PolymerElements/gold-email-input#~1.0.6",
    "paper-card": "PolymerElements/paper-card#~1.0.8",
    "paper-spinner": "PolymerElements/paper-spinner#~1.0.4",
    "social-element": "aengusbates/social-element#~0.0.2",
    "overlay-element": "aengusbates/overlay-element#~0.0.2"
  },
  "devDependencies": {
    "iron-component-page": "PolymerElements/iron-component-page#^1.0.0",
    "web-component-tester": "*"
  }
}
